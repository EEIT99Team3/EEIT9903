<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="isstatement.error" class="org.springframework.web.servlet.view.InternalResourceView">
   <property name="url" value="/pages/IncomeStatement.jsp"></property>
</bean>

<bean id="isstatement.success" class="org.springframework.web.servlet.view.InternalResourceView">
   <property name="url" value="/pages/IncomeStatement.jsp"></property>
</bean>

<bean id="bsstatement.error" class="org.springframework.web.servlet.view.InternalResourceView">
   <property name="url" value="/pages/BalanceSheet.jsp"></property>
</bean>

<bean id="bsstatement.success" class="org.springframework.web.servlet.view.InternalResourceView">

</bean>

<bean id="cfstatement.error" class="org.springframework.web.servlet.view.InternalResourceView">
   <property name="url" value="/pages/CashFlowStatement.jsp"></property>
</bean>

<bean id="cfstatement.success" class="org.springframework.web.servlet.view.InternalResourceView">
   <property name="url" value="/pages/CashFlowStatement.jsp"></property>
</bean>

<bean id="backendlogin.error" class="org.springframework.web.servlet.view.InternalResourceView">
   <property name="url" value="/pages/administratorlogin.jsp"></property>
</bean>

<bean id="backendlogin.success" class="org.springframework.web.servlet.view.InternalResourceView">
   <property name="url" value="/pages/backendcheck/administrator.jsp"></property>
